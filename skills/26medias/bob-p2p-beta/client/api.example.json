{
    "apis": [
        {
            "id": "echo-api-v1",
            "name": "Echo API",
            "description": "Simple echo service for testing",
            "version": "1.0.0",
            "endpoint": "/echo",
            "method": "POST",
            "handler": "./handlers/echo.js",
            "pricing": {
                "amount": 0.01,
                "unit": "per-call"
            },
            "capacity": {
                "concurrent": 5,
                "queueMax": 20,
                "queueTimeout": 60
            },
            "execution": {
                "estimatedDuration": 1,
                "maxDuration": 10,
                "resultRetention": 3600
            },
            "schema": {
                "request": {
                    "type": "object",
                    "properties": {
                        "message": {
                            "type": "string",
                            "maxLength": 1000,
                            "description": "Message to echo back"
                        }
                    },
                    "required": ["message"]
                },
                "response": {
                    "type": "object",
                    "properties": {
                        "echo": {
                            "type": "string"
                        },
                        "timestamp": {
                            "type": "string"
                        }
                    }
                }
            },
            "category": ["testing", "utility"],
            "tags": ["echo", "test", "simple"]
        },
        {
            "id": "runware-text-to-image-v1",
            "name": "Runware Image Generator",
            "description": "Generate images from text prompts using Runware AI",
            "version": "1.0.0",
            "endpoint": "/runware/text-to-image",
            "method": "POST",
            "handler": "../api-demo/runware/handlers/text-to-image.js",
            "pricing": {
                "amount": 0.05,
                "unit": "per-call"
            },
            "capacity": {
                "concurrent": 3,
                "queueMax": 15,
                "queueTimeout": 60
            },
            "execution": {
                "estimatedDuration": 30,
                "maxDuration": 90,
                "resultRetention": 86400
            },
            "schema": {
                "request": {
                    "type": "object",
                    "properties": {
                        "prompt": {
                            "type": "string",
                            "minLength": 1,
                            "maxLength": 1000,
                            "description": "Text description of desired image"
                        },
                        "negativePrompt": {
                            "type": "string",
                            "maxLength": 1000,
                            "description": "What to avoid in the image"
                        },
                        "steps": {
                            "type": "number",
                            "minimum": 10,
                            "maximum": 50,
                            "default": 20,
                            "description": "Number of inference steps"
                        },
                        "width": {
                            "type": "number",
                            "enum": [512, 768, 1024],
                            "default": 1024,
                            "description": "Image width in pixels"
                        },
                        "height": {
                            "type": "number",
                            "enum": [512, 768, 1024],
                            "default": 1024,
                            "description": "Image height in pixels"
                        },
                        "seed": {
                            "type": "number",
                            "description": "Random seed for reproducibility (optional)"
                        }
                    },
                    "required": ["prompt"]
                },
                "response": {
                    "type": "object",
                    "properties": {
                        "imageUrl": {
                            "type": "string",
                            "description": "URL to download the generated image"
                        },
                        "seed": {
                            "type": "number",
                            "description": "Seed used for generation"
                        },
                        "cost": {
                            "type": "number",
                            "description": "Runware API cost"
                        }
                    }
                }
            },
            "category": ["ml", "image", "ai"],
            "tags": ["runware", "image-generation", "text-to-image", "ai"]
        },
        {
            "id": "runware-text-to-video-v1",
            "name": "Runware Video Generator",
            "description": "Generate short videos from text prompts using Runware AI",
            "version": "1.0.0",
            "endpoint": "/runware/text-to-video",
            "method": "POST",
            "handler": "../api-demo/runware/handlers/text-to-video.js",
            "pricing": {
                "amount": 0.25,
                "unit": "per-call"
            },
            "capacity": {
                "concurrent": 2,
                "queueMax": 8,
                "queueTimeout": 60
            },
            "execution": {
                "estimatedDuration": 120,
                "maxDuration": 300,
                "resultRetention": 86400
            },
            "schema": {
                "request": {
                    "type": "object",
                    "properties": {
                        "prompt": {
                            "type": "string",
                            "minLength": 1,
                            "maxLength": 1000,
                            "description": "Text description of desired video"
                        },
                        "duration": {
                            "type": "number",
                            "minimum": 2,
                            "maximum": 10,
                            "default": 3,
                            "description": "Video duration in seconds"
                        },
                        "fps": {
                            "type": "number",
                            "enum": [8, 12, 24],
                            "default": 8,
                            "description": "Frames per second"
                        },
                        "resolution": {
                            "type": "string",
                            "enum": ["512x512", "768x768"],
                            "default": "512x512",
                            "description": "Video resolution"
                        },
                        "seed": {
                            "type": "number",
                            "description": "Random seed for reproducibility (optional)"
                        }
                    },
                    "required": ["prompt"]
                },
                "response": {
                    "type": "object",
                    "properties": {
                        "videoUrl": {
                            "type": "string",
                            "description": "URL to download the generated video"
                        },
                        "duration": {
                            "type": "number",
                            "description": "Actual video duration in seconds"
                        },
                        "resolution": {
                            "type": "string",
                            "description": "Video resolution"
                        },
                        "frameCount": {
                            "type": "number",
                            "description": "Total frames in video"
                        },
                        "seed": {
                            "type": "number",
                            "description": "Seed used for generation"
                        },
                        "cost": {
                            "type": "number",
                            "description": "Runware API cost"
                        }
                    }
                }
            },
            "category": ["ml", "video", "ai"],
            "tags": ["runware", "video-generation", "text-to-video", "ai"]
        }
    ]
}
