{
  "name": "curated-search",
  "version": "1.0.3",
  "description": "Domain-restricted search for OpenClaw",
  "main": "scripts/search.js",
  "bin": {
    "curated-search": "./scripts/search.js"
  },
  "scripts": {
    "search": "node scripts/search.js",
    "crawl": "node scripts/crawl.js",
    "test": "jest --coverage=false",
    "test:unit": "jest --testPathPattern=unit --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "node test/e2e/run.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "bench": "node test/benchmarks/search-bench.js"
  },
  "keywords": [
    "openclaw",
    "search",
    "crawler",
    "curated"
  ],
  "author": "Qrusher",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0",
    "tmp": "^0.2.1",
    "nock": "^13.4.0"
  },
  "dependencies": {
    "jsdom": "^24.0.0",
    "marked": "^12.0.0",
    "minimist": "^1.2.8",
    "minisearch": "^6.3.0",
    "node-fetch": "^2.6.7",
    "robots-parser": "^2.2.1",
    "yaml": "^2.3.4"
  }
}
